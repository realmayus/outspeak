    "type": "module",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.160.0",
        "@aws-sdk/s3-request-presigner": "^3.160.0",
        "@prisma/client": "^4.1.1",
        "@sendgrid/mail": "^7.7.0",
        "@sveltejs/kit": "^1.0.0-next.428",
        "@types/uuid": "^8.3.4",
        "bcrypt": "^5.0.1",
        "cookie": "^0.5.0",
        "iso-639-3": "^3.0.1",
        "langs": "^2.0.0",
        "latlon-geohash": "^2.0.0",
        "locale-code": "^2.0.2",
        "ngeohash": "^0.6.3",
        "uuid": "^8.3.2"
    }
